{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.Authentication": "Information",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Colored, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "fileSizeLimitBytes": 100000,
          "retainedFileCountLimit": 10,
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AxSDKAddress": "10.2.225.33",
  "APIAddress": "http://172.16.15.131:8357",

  //"APIEndPoint": "/home/ocr/tuphap-khaisinh",
  "APIEndPoint": "/AxDES/form/20134-Quyet-Dinh-Ban-An",
  "IsUseAPI": true,
  "DataFolder": {
    //"Path": "\\\\172.16.15.131\\filedata\\KS",
    "Path": "\\\\172.16.15.131\\filedata\\tatc",
    "Filter": "*.pdf"
  },
  "NumOfFileSimulation": 100, // tự động lặp lại DataFolder để đạt được số lượng test mong muốn
  "MaxConcurrency": 10 // số lượng luồng tối đa chạy song song


}
